{
	"name": "rocket-store",
	"version": "0.10.10",
	"description": "A simple, super fast and yet powerfull flat file database.",
	"main": "./dist/cjs/index.js",
	"module": "./dist/mjs/index.js",
	"type": "module",
	"scripts": {
		"test": "node test/test.js",
		"benchmark": "node benchmark/benchmark.js",
		"build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
		"watch": "npm run build -- --watch src"
	},
	"files": [
		"dist",
		"examples",
		"LICENSE",
		"package.json",
		"README.md",
		"src"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/paragi/rocket-store-node.git"
	},
	"keywords": [
		"storage",
		"database",
		"key",
		"value",
		"store",
		"key-value",
		"flat",
		"file",
		"flat-file",
		"file-files",
		"flat file",
		"filebase",
		"database",
		"document",
		"record",
		"serverless",
		"collection",
		"fast"
	],
	"author": "Simon Rig√©t",
	"contributors": [
		"Anton Sychev <anton@sychev.xyz> [https://sychev.xyz]"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/paragi/rocket-store-node/issues"
	},
	"homepage": "https://github.com/paragi/rocket-store-node#readme",
	"dependencies": {
		"glob-to-regexp": ">0.4.0"
	},
	"peerDependencies": {
		"rollup": ">=0.66.0 <2"
	},
	"devDependencies": {
		"jest": "^29.7.0",
		"rollup": "^1.27.10",
		"tsc": "^2.0.4",
		"typescript": "^5.2.2"
	},
	"exports": {
		".": {
			"import": "./dist/mjs/index.js",
			"require": "./dist/cjs/index.js"
